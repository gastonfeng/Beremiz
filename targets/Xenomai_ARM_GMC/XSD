      <xsd:element name="Xenomai_ARM_GMC">
        <xsd:complexType>
          <xsd:attribute name="Compiler" type="xsd:string" use="optional" default="arm-linux-gnueabihf-gcc"/>
          <xsd:attribute name="CFLAGS" type="xsd:string" use="optional" default="-I../arm-linux-gnueabihf/usr/include/xenomai -D_GNU_SOURCE -D_REENTRANT -Wall -pipe -D__XENO__"/>
          <xsd:attribute name="Linker" type="xsd:string" use="optional" default="arm-linux-gnueabihf-gcc"/>
          <xsd:attribute name="LDFLAGS" type="xsd:string" use="optional" default="-lnative -lxenomai -lpthread -lethercat_rtdm -lrtdm"/>
          <xsd:attribute name="XenoConfig" type="xsd:string" use="optional"/>
        </xsd:complexType>
      </xsd:element>
